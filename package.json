{
  "name": "@laqudee/promise-futurable",
  "version": "0.0.1",
  "description": "Building Promises from scratch",
  "type": "module",
  "main": "src/Futurable.ts",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "dev": "tsx --no-warnings examples/useFuturable.ts",
    "dev:statics": "tsx --no-warnings examples/useFuturableStaticMethods.ts",
    "dev:esm": "node -r esm examples/useFuturable.js",
    "build": "rollup -c",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "esm": "^3.2.25",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@types/node": "^20.10.5",
    "es-aggregate-error": "^1.0.11"
  }
}
